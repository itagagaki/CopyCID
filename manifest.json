{
  "manifest_version": 3,

  "name": "__MSG_name__",
  "description": "__MSG_desc__",
  "version": "1.1.2",
  "homepage_url": "https://github.com/itagagaki/CopyCID",
  "author": "Itagaki Fumihiko",
  "default_locale": "en",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "event.js"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["*://*/maps*"],
      "run_at": "document_end",
      "js": ["./page.js"]
    }
  ],
  "permissions": ["contextMenus", "scripting"],
  "host_permissions": ["*://*/maps/*"]
}
